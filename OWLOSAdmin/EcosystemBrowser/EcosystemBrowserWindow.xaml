<Window x:Class="OWLOSAdmin.EcosystemBrowser.EcosystemBrowserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OWLOSAdmin.EcosystemBrowser"
        mc:Ignorable="d"
        Title="EcosystemBrowserWindow" Height="550" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="9*"/>
        </Grid.ColumnDefinitions>

        <TreeView x:Name="BrowserTreeView">
        </TreeView>

        <Grid x:Name="HolderGrid" Grid.Column="1" Background="#FFE7E8EC">
            <Grid.RowDefinitions>
                <RowDefinition Height="35"/>
                <RowDefinition Height="100*"/>
            </Grid.RowDefinitions>
            <StackPanel x:Name="BrowserTabsPanes" Orientation="Horizontal">
                
            </StackPanel>
            <Grid x:Name="BrowserGrid" Grid.Row="2">

            </Grid>
        </Grid>

    </Grid>
</Window>
