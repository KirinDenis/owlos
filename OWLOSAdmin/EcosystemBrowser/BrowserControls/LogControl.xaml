<UserControl x:Class="OWLOSThingsManager.EcosystemBrowser.BrowserControls.LogControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OWLOSThingsManager.EcosystemBrowser.BrowserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">

    <ScrollViewer x:Name="GlobalOutLogScrollViewer" VerticalAlignment="Stretch" HorizontalScrollBarVisibility="Auto" Margin="10,0,0,0" FontFamily="Courier New" SnapsToDevicePixels="True">
        <!-- <TextBlock  x:Name="GlobalOutLogTextBlock" VerticalAlignment="Bottom"  Foreground="{DynamicResource {x:Static SystemColors.MenuTextBrushKey}}" Text=".,34" FontFamily="Courier New"/> -->
    <VirtualizingStackPanel Name="logPanel" ScrollViewer.CanContentScroll="True" >
    </VirtualizingStackPanel>
    </ScrollViewer>
</UserControl>
