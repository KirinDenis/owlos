<Window x:Class="OWLOSAirQuality.Frames.LogFrame"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OWLOSAirQuality.Frames" xmlns:huds="clr-namespace:OWLOSAirQuality.Huds"
        mc:Ignorable="d"
        Title="Log Frame" Width="1920" Height="1080">
    <Grid x:Name="MainGrid" Width="1920" Height="1080">
        <Grid x:Name="LogHolderGrid" Grid.Column="0" Grid.Row="1">
            <huds:FrameBackgroundControl TitleFrame="LOG" DescriptionFrame="Air Quality" QueryInterval="x1 sec"/>
            <Grid x:Name="LogGrid" Margin="60,120,60,145" Width="1800"/>
        </Grid>
    </Grid>
</Window>
